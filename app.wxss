/**app.wxss**/
page{
  background: #F3F3F3;
  width: 100%;
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.button-orange-center {
  position:fixed;
  left: 50%;
  margin-left: -180rpx;
  bottom: 50rpx;
  right: 225rpx;
  z-index:100;
  border-radius: 50rpx;
  width: 360rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-image:linear-gradient(135deg, #FA8534, #E6660E);
  box-shadow: 0 20rpx 20rpx -10rpx #FA8534; 
  color: #fff;
  font-size:50rpx;
  padding:0;
}
 .cell-style{
  background:rgba(255,255,255,1);
  border-radius: 20rpx;
  box-shadow:10px 10px 20px rgba(136,136,136,0.15);
  width: 710rpx;
  margin-left: 20rpx; 
  margin-top: 20rpx;
}
/* 横线 */
.line-style {
  height:1px;
  background:#B2B2B2;
  overflow:hidden;
  opacity:0.6;
}